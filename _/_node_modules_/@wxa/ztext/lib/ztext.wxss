.ztext {
    word-wrap: break-word;
    line-height: 1.6;
}

.h1,.h2,.h3,.h4,.h5,.h6 {
    margin-top: 1.66em;
    margin-bottom: 0.83em;
    font-size: 1.2em;
    line-height: 1.5;
}

.hr {
    margin: 4em auto;
    width: 240px;
    max-width: 100%;
    border: none;
    border-top: 1px solid #d3d3d3;
}

.pre {
    margin: 1em 0;
    padding: calc(0.8 / 0.9em);
    font-size: 0.9em;
    word-wrap: normal;
    white-space: pre;
    background-color: #f6f6f6;
    border-radius: 4px;
    overflow: auto;
}

.u {
    border-bottom: 1px dashed #808080;
}

.code {
    margin: 0 2px;
    padding: 3px 4px;
    border-radius: 3px;
    background-color: #f6f6f6;
    font-family: Menlo,Monaco,Consolas,Andale Mono,lucida console,Courier New,monospace;
}

.pre .code {
    margin: 0;
    padding: 0;
}

.ui-block,.figure,.blockquote,.highlight,.h1,.h2,.h3,.h4,.h5,.h6,.ul,.ol,.p {
    margin: 1em 0;
}

.blockquote {
    padding-left: 1em;
    color: #808080;
    border-left: 4px solid #d3d3d3;
}

.img {
    max-width: 100%;
}

.origin_image,.content_image {
    display: block;
    margin: 0 auto;
}

.origin_image.size-small {
    max-width: 40%;
}

.eeimg {
    max-width: 100%;
}

.eeimg-inline {
    display: inline-block;
    margin: 0 3px;
    vertical-align: middle;
}

.a {
    text-decoration: none;
}

.ol,.ul {
    margin: 1em 0;
    padding: 0;
    padding-left: 1.6em;
}

.ol .ol,.ol .ul,.ul .ol,.ul .ul {
    margin: 0;
}